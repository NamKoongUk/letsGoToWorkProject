<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Employee">
	<!-- <resultMap id="내가쓸 이름" type="vo풀네임">
		<id column="" property="PK"/>
		<result column="" property="필드 명"/>
	</resultMap>
	
	쿼리문 작성
	<select id="구분이름" parameterType="매개변수 타입"> </select> -->
	<resultMap id="loginResultSet" type="com.kh.lgtw.employee.model.vo.Employee">
		<id column="EMPNO" property="empNo"/>
		<result column="EMP_ID" property="empId"/>
		<result column="EMP_PWD" property="empPwd"/>
		<result column="EMP_NAME" property="empName"/>
	</resultMap>
	
	<select id="loginCheck" resultMap="loginResultSet" parameterType="Employee">
		SELECT *
		FROM EMPLOYEE
		WHERE STATUS = 'Y' 
	</select>
</mapper>
